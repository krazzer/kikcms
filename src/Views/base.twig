<!DOCTYPE html>
<html lang="{{ langCode }}">
<head>
    <meta charset="utf-8">
    <title>{{ cmsTitlePrefix ? cmsTitlePrefix ~ ' - ' }}{% block title title %}</title>
    <link rel="shortcut icon" href="/cmsassets/favicon.ico"/>
    <link rel="apple-touch-icon" href="/cmsassets/icons/kikcms-icon-76.png">
    <link rel="apple-touch-icon" sizes="76x76" href="/cmsassets/icons/kikcms-icon-76.png">
    <link rel="apple-touch-icon" sizes="120x120" href="/cmsassets/icons/kikcms-icon-120.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/cmsassets/icons/kikcms-icon-152.png">
    {{ css('//fonts.googleapis.com/css?family=Lato:300,400,700') }}
    {{ css('cmsassets/css/vendor.css') }}
    {{ css('cmsassets/css/cms.css') }}
    {{ assets.outputCss() }}
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="robots" content="noindex">
    {% block afterHead '' %}
</head>
<body class="{% block bodyClass '' %}">
<div id="overlay"></div>
{% block content %}{% endblock %}
{{ js('cmsassets/js/vendor.js') }}
{{ js('cmsassets/js/cms.js') }}
<data id="kikCmsJsSettings" value='settings' style="display: none;">{{ jsSettings|json_encode|raw }}</data>
{{ assets.outputJs() }}
</body>
</html>