{
    "homepage": "https://kikcms.com",
    "license": "MIT",
    "name": "kiksaus/kikcms",
    "config": {
        "platform": {
            "php": "7.4",
            "ext-zip": "master",
            "ext-sockets": "master",
            "ext-phalcon": "4.0"
        },
        "platform-check": false
    },
    "description": "A CMS based on the Phalcon Framework",
    "require": {
        "twig/twig": "~1.0",
        "google/apiclient": "^2.0",
        "nette/php-generator": "^3.1",
        "martijnc/php-csp": "^1.0",
        "google/recaptcha": "^1.2",
        "kiksaus/kikcms-core": "dev-phalcon4"
    },
    "autoload": {
        "psr-4": {
            "KikCMS\\": "src"
        }
    },
    "require-dev": {
        "codeception/codeception": "^4.0",
        "codeception/c3": "2.*",
        "donatj/mock-webserver": "dev-master",
        "codeception/module-webdriver": "^1.0.0",
        "codeception/module-db": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-filesystem": "^1.0.0",
        "codeception/module-phalcon": "^1.0.0",
        "codeception/module-cli": "^1.0.0",
        "codeception/module-phalcon4": "^1.0"
    }
}
